{
    "version": 2,
      "functions": {
        "/api/**/*": {
          "maxDuration": 25 
        }
      },
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }