generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
  id                    Int                     @id @default(autoincrement())
  first_name            String
  last_name             String
  email                 String                  @unique
  password              String
  phone                 String
  address               String
  joined_at             DateTime                @default(now())
  user_events           Json?
}
